package bit.basic.obj4;

public class Parent {
	
	public Parent() {
		System.out.println("parent 생성자");
	}
	
	public void sayHi() {
		System.out.println("parent sayHi");
	}

}

class Child extends Parent{
	
	public Child() {
		System.out.println("child 생성자");
	}
	
	@Override
	public void sayHi() {
		System.out.println("child sayHi");
	}
}

package bit.basic.obj1;

public class Child extends Parent{
	
	public Child() {
//		super();
		this.val = "child";
			
	}
	
	public Child(String val) {
		
		this.val = val;
	}
	
	public static void main(String[] args) {
		Parent p = new Child("아빠");
		System.out.println(p.val1);
		System.out.println(p.val);
	}

}
